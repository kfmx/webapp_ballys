{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "strict": true,
      "jsx": "preserve",
      "jsxImportSource": "vue",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["assets/*"]
      },
      "types": ["webpack-env"]
    },
    "include": ["assets/**/*.ts", "assets/**/*.tsx", "assets/**/*.vue"],
    "exclude": ["node_modules"]
}
